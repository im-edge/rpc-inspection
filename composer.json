{
    "name": "imedge/web-rpc-inspection",
    "description": "IMEdge Web RPC Inspection library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\Web\\Rpc\\Inspection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\Web\\Rpc\\Inspection\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "imedge/json": ">=0.1"
    },
    "require-dev": {
        "gipfl/icingaweb2": "^0.10",
        "ipl/html": "^0.8",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.3 || ^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^12.0.8"
    }
}
